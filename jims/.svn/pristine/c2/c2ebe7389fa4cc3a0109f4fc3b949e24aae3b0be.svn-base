<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="mainDAO">


 	    <!-- 메인화면 소속급식기관목록  -->
	<select id="selectMlsvcInsttSelList" parameterType="java.util.HashMap" resultType="egovMap">
		/* mainDAO.selectMlsvcInsttSelList 소속급식기관 목록 */
		SELECT 
			INSTT_CD
			, INSTT_NM 
		  FROM INSTT
		 WHERE USE_YN = 'Y'
		   AND INSTT_CD IN ( SELECT b.INSTT_CD 
						       FROM USER a, INSTT_USER b 
						      WHERE a.USER_ID = b.USER_ID 
						        AND a.USER_ID = #{userId}
						        AND b.IOFIC_STAT_CD ='C13002')
	</select>   


 	
</mapper>
