/* 달력 */
.month{width:100%; overflow:hidden; border:1px solid #dcdcdc;}
.month > ul{width:100%; display:flex;}
.month > ul > li{float:left; background:#ffffff; box-sizing:border-box; width:100%; text-align:center; border:1px solid #dcdcdc; position:relative;}
.month .seven_days{height:30px; line-height:28px;}
.month .seven_days li{ font-size:14px; background:#476683;}
.month .seven_days li p{color:#ffffff;}
.month .week li:first-child{background:#ffffff!important;}
.month .week li:last-child{background:#ffffff!important;}
.month .week.on li{background:#d0d0d0;}
.month .week li{padding:30px 10px 10px 10px; position:relative;}
.month .week li .day{position:absolute; top:10px; left:10px; color:#555555;}
.month .week li .day.prev,
.month .week li .day.next{color:#a2a2a2;}
.month .week li .time{display:block; margin-bottom:5px; color: #448aca; background:#eeeeee; height:30px; line-height:28px;}
.month .week li .time.break{color:#e66000; }
.month .week li .time.lunch{color:#3e3d3d; background:#e4e4e4; margin-top:5px;}
.month .week li .time.dinner{color:#1c6cb7;margin-bottom:0;}
.month .week li .time.on{background:#ffc800; font-weight:bold;}

