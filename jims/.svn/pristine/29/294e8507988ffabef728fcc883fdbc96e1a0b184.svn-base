/* form */



input[type="text"]{border:1px solid #dcdcdc;height:30px;line-height:32px;padding:0 4px;width:30px;vertical-align:middle; box-sizing:border-box;}
input[type="password"]{border:1px solid #dcdcdc;height:30px;line-height:32px;padding:0 4px;width:30px;vertical-align:middle; box-sizing:border-box;}
select{border:1px solid #dcdcdc;height:30px;  box-sizing:border-box;}
select { -webkit-appearance: none; /* 네이티브 외형 감추기 */  -moz-appearance: none;  appearance: none; background:#ffffff url(../../images/main/select_arrow.png) no-repeat 95% 50%; /* 화살표 모양의 이미지 */ padding:0 4px;} 
select::-ms-expand { display: none; } 
textarea{width:100%;border:1px solid #dcdcdc;padding:8px 11px;box-sizing:border-box;}

.inp-radio{display:inline-block;height:15px;width:auto;}
.inp-radio input[type = 'radio'] {opacity:0;position:absolute; }
.inp-radio label {position:relative;display:block;height:15px;line-height:15px;width:auto; padding:0 0 0 20px;margin-right:16px;font-size:14px;color:#333;cursor:pointer;background: url('../../images/main/bg_radio.png') no-repeat 0 0;letter-spacing:-0.1em; }
.inp-radio input[type = 'radio']:checked + label {background: url('../../images/main/bg_radio.png') no-repeat 0 100%;background-size:15px auto;}


/* input[type = 'checkbox'] {opacity:0;position:absolute; } */
input[type = 'checkbox'] + label {position:relative;display:block;height:16px;line-height:15px;width:18px; font-size:14px;color:#333;cursor:pointer;background: url('../../images/main/bg_check_01.png') no-repeat 0 0;letter-spacing:-0.1em;  margin:0 auto;}
input[type = 'checkbox']:checked + label {background: url('../../images/main/bg_check_01.png') no-repeat 0 100%; background-size:15px auto;}

.check_agree1{position:relative;display:inline-block;color:#333;font-size:14px;line-height: 24px;}
.check_agree1 label {display:block;padding:0 0 0 20px;cursor:pointer;font-size:14px;line-height: 15px;font-weight:normal;color:#666;}
.check_agree1 label:after {content:'';display:block;width:18px;height:16px;position: absolute;left: 0; margin-top:-15px;font-size: 0;background:url("../../images/main/bg_check_01.png") no-repeat 0 0px;}
.check_agree1 input[type="checkbox"] {opacity: 0;width: 0;height: 0;margin-left: -20px;position: absolute;}
.check_agree1 input[type="checkbox"]:checked + label::after {background:url("../../images/main/bg_check_01.png") no-repeat 0 100%;}
.check_agree1 input[type = 'checkbox'] + label{width:auto;}

.check_agree2{position:relative;display:block;color:#333;font-size:14px;line-height: 24px;}
.check_agree2 label {display:block;padding:0 0 0 20px;cursor:pointer;font-size:14px;line-height: 15px;font-weight:normal;color:#666;}
.check_agree2 label:after {content:'';display:block;width:18px;height:16px;position: absolute;left: 0; margin-top:-15px;font-size: 0;background:url("../../images/main/bg_check_01.png") no-repeat 0 0px;}
.check_agree2 input[type="checkbox"] {opacity: 0;width: 0;height: 0;margin-left: -20px;position: absolute;}
.check_agree2 input[type="checkbox"]:checked + label::after {background:url("../../images/main/bg_check_01.png") no-repeat 0 100%;}
.check_agree2 input[type = 'checkbox'] + label{width:auto;}

/* column */
