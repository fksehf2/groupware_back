#header .h_select{position:absolute; left:250px; top:50%; transform:translateY(-50%);}
#header .h_select .sel_title{display:inline-block; font-size:16px; color:#3b5874; vertical-align:middle;}
#header .h_select select{height:34px; border:2px solid #476683; padding:0 17px; width:200px; border-radius:20px; margin-left:17px;}
#header .util{position:absolute; right:40px;top:50%; transform:translateY(-50%);width:500px;text-align:right;}
#header .util .txt{color:#555555;font-size:16px; position:relative;}
#header .util .txt::before{display:inline-block; content:""; background:url('../../images/main/log_face.png') center no-repeat; width:44px; height:44px; position:absolute; left: -49px; top: -14px;}
