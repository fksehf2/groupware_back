/* header */ 
#header{position:fixed; z-index:10;width:100%; height:70px; top:0;  background:#ffffff; border-bottom:1px solid #d2d2d2;     min-width: 1520px;}
#header h1{position:absolute; left:15px; top:50%; transform:translateY(-50%);}
