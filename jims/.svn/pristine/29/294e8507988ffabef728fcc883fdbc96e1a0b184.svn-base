.t-area{border:1px solid #dcdcdc; padding:10px; background:#ffffff; box-sizing:border-box;}
.t-area textarea{border:none; height:100%;}

.file_box{width:100%; overflow:hidden; display:block; box-sizing:border-box; height:auto;}
input.upload_text {/*읽기전용 인풋텍스트*/ float:left; width:200px;/* 버튼 포함 전체 가로 길이*/ height:30px; line-height:19px; padding:0 3px; border:1px solid #dcdcdc;}
div.upload-btn_wrap input.input_file {/*파일찾기 폼 투명하게*/ position:absolute; top:0; right:0; cursor:pointer; opacity:0; filter:alpha(opacity=0); -ms-filter:"alpha(opacity=0)"; -moz-opacity:0;}
div.upload-btn_wrap {/*버튼테두리 감싼 div*/ overflow:hidden; position:relative; float:left; width:70px;/*width, height 값은 button(찾아보기)값과 같아야함 */ height:34px; padding-left:3px; }
div.upload-btn_wrap button {/*버튼 div*/ width:70px; height:34px; background:#333; border:1px solid #333; color:#fff; font-size:15px; }


/* paging */
.paging{position:relative;text-align:center;color:#333; margin-top:-1px;padding:6px 0;line-height:23px}
.paging a{cursor:pointer; display:inline-block; width:35px; height:35px; border:1px solid #eeeeee; line-height:33px; box-sizing:border-box; color:#555555; font-size:16px;}
.paging p{cursor:pointer; display:inline-block; width:35px; height:35px; border:1px solid #eeeeee; line-height:33px; box-sizing:border-box; color:#555555; font-size:16px;}
.paging .first, .paging .prev, .paging .next, .paging .last{width:35px;height:35px;padding:0;line-height:0;font-size:0;text-indent:-99px;border:none;vertical-align:top;border:1px solid #eeeeee;}
.paging .first{background:#ffffff url('../../images/main/btn_page_first.png') no-repeat 50% 50%;}
.paging .prev{background:#ffffff url('../../images/main/btn_page_prev.png') no-repeat 50% 50%;}
.paging .next{background:#ffffff url('../../images/main/btn_page_next.png') no-repeat 50% 50%;}
.paging .last{background:#ffffff url('../../images/main/btn_page_last.png') no-repeat 50% 50%;}
.paging strong, .paging a{display:inline-block;vertical-align:top;font-weight:normal;}
/* .paging strong, .paging a:hover, .paging a:focus{color:#bc3231;} */
/* .paging p{position:absolute;right:9px;top:20px;font-size:13px;color:#333;} */
.btn_page{background:#ffffff;}
.btn_page:hover{background:#476683; color:#ffffff;}
.btn_page.on{border:1px solid #476683;}


/* box */
.box_tab{position:relative; margin-top:8px; border-bottom:2px solid #314960;}
.box_tab ul{position:relative; overflow:hidden;  width:calc(100% - 10%);}
.box_tab li{position:relative;width:10%;height:35px; float:left; box-sizing:border-box;}
.box_tab li a{display:block;height:35px;line-height:35px; border-left:1px solid #dcdcdc; border-top:1px solid #dcdcdc; background:#fff;padding:0 25px 0 13px ;color:#333;font-size:13px;letter-spacing:-0.02em; box-sizing:border-box;}
.box_tab li:last-child a{border-right:1px solid #dcdcdc;}
.box_tab li:nth-child(10n) a{border-right:1px solid #dcdcdc;}

.box_tab li button{position:absolute;display:block;width:12px;height:12px;background:url('../../images/main/btn_delete.png') no-repeat 50% 50%;right:13px; top:12px;font-size:0;}
.box_tab li.on a, .box_tab li a:hover{background:#476683;color:#fff;font-weight:bold;text-decoration:none;}
.box_tab li.on button, .box_tab li a:hover + button{background:url('../../images/main/btn_delete_on.png') no-repeat 50% 50%;}
.box_tab .btn_close_all{position:absolute; display:inline-block; bottom:0; right:0; height:35px;line-height:35px;  width:10%;right:0px; top:0px; text-align:right; color:#000; font-size:13px;font-weight:bold; box-sizing:border-box; text-align:left;  padding-left:13px; background:#aaaaaa; color:#ffffff; border:1px solid #dcdcdc; border-bottom:0; bottom:10px;}
.box_tab .btn_close_all::after{display:inline-block; content:""; background:url('../../images/main/btn_close.png') center no-repeat; width:12px; height:12px; position:absolute; top:50%; transform:translateY(-50%); right:13px;}



/* location */
.location{height:30px;padding:7px 0 0 0px;font-size:14px;}
.location span{margin:0 6px;font-size:14px;color:#303030;vertical-align:unset;font-weight:bold;}
.location strong{margin:0 6px;font-size:14px;color:#303030;vertical-align:unset;}
.location .home{font-size:0;background:url('../../images/main/ico_home.png') no-repeat 50% 50%;width:14px;height:12px;display:inline-block; vertical-align:middle;}


/* txt */
.tit_ty1{position:relative; font-size:23px;font-weight:bold;color:#666666;letter-spacing:-1px; line-height: 26px;  margin-top:30px; display:inline-block;}
.tit_ty2{position:relative; font-size:18px;font-weight:bold;color:#666666;letter-spacing:-1px; line-height: 26px;  margin-top:30px; display:inline-block;}
.tit_ty2::before{display:inline-block; content:"-"; color:#666666;  font-weight:bold; margin-right:10px;}
